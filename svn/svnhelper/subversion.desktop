[Desktop Entry]
ServiceTypes=inode/directory,all/all
Actions=Update;Commit;Add;Delete;
X-KDE-Submenu=Subversion

[Desktop Action Update]
Name=Update
Icon=subversion
Exec=kio_svn_helper -u %U

[Desktop Action Commit]
Name=Commit
Icon=subversion
Exec=kio_svn_helper -c %U

[Desktop Action Add]
Name=Add to the repository
Icon=subversion
Exec=kio_svn_helper -a %U

[Desktop Action Delete]
Name=Delete from the repository
Icon=subversion
Exec=kio_svn_helper -d %U

